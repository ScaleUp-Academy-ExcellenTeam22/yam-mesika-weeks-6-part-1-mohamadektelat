def is_positive(number):
    """
    This function returns true if the number is positive else returns false.
    :param: number.
    :return:returns true if the number is positive else returns false.
    """
    return number >= 0


# Press the green button in the gutter to run the script.
if __name__ == '__main__':
    input_list = list(map(int, input("\nEnter the numbers : ").strip().split(',')))
    to_return_list = list(filter(is_positive, input_list))
    print(to_return_list)

